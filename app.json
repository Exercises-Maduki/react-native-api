{
  "expo": {
    "name": "api",
    "slug": "api",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
  "image": "./assets/logo.png",
  "resizeMode": "contain",
  "backgroundColor": "#000000",
  "imageStyle": {
    "width": 100,
    "height": 100
  },
  "statusBar": {
    "style": "light-content",
    "backgroundColor": "#000000"
  }
}
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
